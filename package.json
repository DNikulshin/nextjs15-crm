{
  "name": "nextjs-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev  --turbopack --experimental-https",
    "dev:host": "next dev --turbopack  -H 192.168.1.111 --experimental-https",
    "build": "npm run prisma:init && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:init": "npx prisma generate && npx prisma migrate dev deploy"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@tanstack/react-query": "^5.66.10",
    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "jose": "^6.0.8",
    "next": "15.2.0",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "prisma": "^6.4.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
